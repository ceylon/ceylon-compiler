package com.redhat.ceylon.compiler.test.structure.attribute;

class ClassVariable extends .ceylon.language.Object {
    private String publicvar;
    private String capturedvar;
    
    private final void m() {
        setCapturedvar("new value");
    }
    
    ClassVariable() {
        String uncapturedvar;
        setPublicvar("");
        uncapturedvar = "";
        setCapturedvar("");
    }
    
    public String getPublicvar() {
        return this.publicvar;
    }
    
    public void setPublicvar(String publicvar) {
        this.publicvar = publicvar;
    }
    
    private String getCapturedvar() {
        return this.capturedvar;
    }
    
    private void setCapturedvar(String capturedvar) {
        this.capturedvar = capturedvar;
    }
}